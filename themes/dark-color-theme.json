  "tokenColors": [
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#F87272F2"
      }
    },
    {
      "scope": [
        "meta.brace",
        "punctuation",
        "keyword.operator.class",
        "keyword.operator.type.annotation",
        "storage.type.function.arrow"
      ],
      "settings": {
        "foreground": "#586366"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
      ],
      "settings": {
        "foreground": "#75898f"
      }
    },
    {
      "name": "directive definition",
      "scope": [
        "entity.name.other.preprocessor",
        "entity.name.function.preprocessor"
      ],
      "settings": {
        "foreground": "#968ba2"
      }
    },
    {
      "name": "keywords",
      "scope": [
        "keyword",
        "keyword.control.directive",
        "storage.modifier.async",
        "storage.type.function"
      ],
      "settings": {
        "foreground": "#c27a8d"
      }
    },
    {
      "name": "components",
      "scope": [
        "support.class.component"
      ],
      "settings": {
        "foreground": "#628fc3"
      }
    },
    {
      "scope": [
        "storage",
        "keyword.const.go",
        "keyword.type.go",
        "keyword.struct.go",
        "keyword.function.go",
        "keyword.other.declaration-specifier",
        "entity.name.tag",
        "entity.name.section.markdown",
        "entity.name.section.group-title"
